<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/home_list_games_caixa_fragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="br.com.minhasortemegasena.ui.games.megasena.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
    </fragment>

    <fragment
        android:id="@+id/screenResultFragment"
        android:name="br.com.minhasortemegasena.ui.games.megasena.ScreenResultContestsFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_screen_result_contest">
    </fragment>


    <fragment
        android:id="@+id/support_fragment"
        android:name="br.com.minhasortemegasena.SupportFragment"
        tools:layout="@layout/support_fragment"
        android:label="support_fragment"/>

    <fragment
        android:id="@+id/home_list_games_caixa_fragment"
        android:name="br.com.minhasortemegasena.ui.home.HomeFragment"
        tools:layout="@layout/home_list_games_caixa_fragment"
        >
        <action
            android:id="@+id/action_home_list_games_to_megasena_palpites_fragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/home_list_games_caixa_fragment"
            />
        <action
            android:id="@+id/action_home_list_games_to_lotofacil_palpites_fragment"
            app:destination="@id/lotofacil_palpite_fragment"
            />
        <action
            android:id="@+id/action_home_list_games_to_federal_palpites_fragment"
            app:destination="@id/federal_palpite_fragment"
            />
    </fragment>

    <fragment
        android:id="@+id/home_list_results"
        android:name="br.com.minhasortemegasena.ui.resultlist.ResultListFragment"
        tools:layout="@layout/home_list_results_caixa_fragment"
        >
        <action
            android:id="@+id/action_list_results_to_megasena_results_fragment"
            app:destination="@id/screenResultFragment"
            />
        <action
            android:id="@+id/action_list_results_to_lotofacil_results_fragment"
            app:destination="@+id/fragment_lotofacil_result_contest"
            />
        <action
            android:id="@+id/action_list_results_to_federal_results_fragment"
            app:destination="@+id/fragment_federal_result_contest"
            />
    </fragment>

    <fragment
        android:id="@+id/lotofacil_palpite_fragment"
        android:name="br.com.minhasortemegasena.ui.games.lotofacil.LotofacilPalpiteFragment"
        tools:layout="@layout/lotofacil_palpite_fragment"
        />
    <fragment
        android:id="@+id/fragment_lotofacil_result_contest"
        android:name="br.com.minhasortemegasena.ui.games.lotofacil.LotofacilResultFragment"
        tools:layout="@layout/fragment_lotofacil_result_contest"
        />
    <fragment
        android:id="@+id/fragment_federal_result_contest"
        android:name="br.com.minhasortemegasena.ui.games.federal.FederalResultFragment"
        tools:layout="@layout/fragment_federal_result_contest"
        />
    <fragment
        android:id="@+id/federal_palpite_fragment"
        android:name="br.com.minhasortemegasena.ui.games.federal.FederalPalpiteFragment"
        tools:layout="@layout/federal_palpite_fragment"
        />

</navigation>